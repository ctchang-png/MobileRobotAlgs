classdef mrplSystem < handle
    
    properties
        rIF
        model
        controller
        poseEstimator
        logger
        executor
    end
    
    methods
        function obj = mrplSystem(rIF)
            obj = obj@handle;
            obj.rIF = rIF;
            obj.model = Model();
            obj.poseEstimator = PoseEstimator(obj.model);
            obj.logger = Logger(true);
            obj.executor = Executor(obj.model);
            
    end
    
end